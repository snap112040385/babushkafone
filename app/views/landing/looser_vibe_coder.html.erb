        }

        .btn {
            display: inline-flex;
            align-items: center;
            justify-content: center;
            gap: 0.6rem;
            padding: 1.05rem 2.8rem;
            background: linear-gradient(115deg, var(--accent-sun), var(--accent-warm), var(--accent-neon));
            color: #0b0502;
            font-weight: 700;
            text-decoration: none;
            border-radius: 999px;
            border: none;
            cursor: pointer;
            transition: transform 0.25s ease, box-shadow 0.25s ease;
            font-size: 1.05rem;
            box-shadow: 0 20px 45px rgba(244, 211, 94, 0.35);
        }

        .btn:hover {
            transform: translateY(-3px) scale(1.01);
            box-shadow: 0 30px 65px rgba(244, 211, 94, 0.4);
