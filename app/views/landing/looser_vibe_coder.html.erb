            border: 1px solid var(--border-color);
            border-radius: 26px;
            padding: 2.6rem;
            transition: transform 0.35s ease, border-color 0.35s ease, box-shadow 0.35s ease;
            position: relative;
            overflow: hidden;
        }

        .card::after {
            content: "";
            position: absolute;
            inset: 0;
            background: radial-gradient(circle at top right, rgba(244, 211, 94, 0.18), transparent 45%);
            opacity: 0;
            transition: opacity 0.4s ease;
            pointer-events: none;
