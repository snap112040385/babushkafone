            border: 1px solid rgba(255, 255, 255, 0.15);
            border-radius: 28px;
            padding: 3rem;
            text-align: center;
            max-width: 860px;
            margin: 0 auto;
            box-shadow: inset 0 0 40px rgba(53, 251, 214, 0.05);
        }
        
        .calc-row {
            display: flex;
            justify-content: space-around;
            margin: 2rem 0;
            flex-wrap: wrap;
            gap: 1rem;
        }
        
        .calc-item {
             font-size: 1.2rem;
             text-transform: uppercase;
             letter-spacing: 0.08em;
        }
        
        .calc-result {
            font-size: 1.6rem;
            font-weight: 800;
            color: var(--accent-neon);
        }

        /* --- Final CTA --- */
        .final-cta {
            text-align: center;
            background: linear-gradient(180deg, rgba(8, 10, 16, 0.95) 0%, rgba(5, 6, 7, 1) 100%);
