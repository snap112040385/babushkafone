            content: "";
            position: absolute;
            width: 400px;
            height: 400px;
            background: var(--accent-neon);
            filter: blur(120px);
            opacity: 0.25;
            top: -10%;
            left: 10%;
        }

        .lead-form {
            max-width: 520px;
            margin: 3rem auto 0;
            display: flex;
            flex-direction: column;
            gap: 1rem;
        }

        .input-field {
            width: 100%;
            padding: 1.05rem 1.2rem;
            background-color: rgba(14, 20, 32, 0.85);
            border: 1px solid rgba(255, 255, 255, 0.15);
            border-radius: 16px;
            color: var(--text-primary);
            font-size: 1rem;
            font-family: 'Space Grotesk', sans-serif;
        }
        
        .input-field:focus {
            outline: none;
            border-color: var(--accent-neon);
            box-shadow: 0 0 0 3px rgba(53, 251, 214, 0.2);
        }
        
        .form-note {
             margin-top: 1rem;
             font-size: 0.9rem;
             color: var(--text-secondary);
             letter-spacing: 0.08em;
             text-transform: uppercase;
        }

        /* --- –ê–¥–∞–ø—Ç–∏–≤–Ω–æ—Å—Ç—å --- */
        @media (max-width: 968px) {
            .hero-content {
                grid-template-columns: 1fr;
                text-align: center;
            }
            .hero-visual {
                order: -1;
                margin-bottom: 2rem;
            }
            h1 { font-size: clamp(2.2rem, 8vw, 3rem); }
            .cta-subtext { letter-spacing: 0.1em; }
        }
        @media (max-width: 600px) {
            .calc-row {
                flex-direction: column;
            }
            .card {
                padding: 2rem;
            }
        }

    </style>
</head>
