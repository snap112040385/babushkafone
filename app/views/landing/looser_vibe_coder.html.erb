            --bg-secondary: #0e0f16;
            --bg-card: rgba(14, 20, 32, 0.85);
            --bg-card-light: rgba(255, 255, 255, 0.04);
            --text-primary: #f5f5f1;
            --text-secondary: rgba(243, 243, 238, 0.75);
            --accent-sun: #f4d35e;
            --accent-neon: #35fbd6;
            --accent-warm: #ff5c5c;
            --border-color: rgba(255, 255, 255, 0.18);
            --grid-line: rgba(255, 255, 255, 0.05);
            --container-width: 1200px;
            --blur: 80px;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
