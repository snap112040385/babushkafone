<%# Authentication Modals %>
<div data-login-modal>
  <%= render "shared/login_modal" %>
</div>

<div data-signup-modal>
  <%= render "shared/signup_modal" %>
</div>

<script>
  function openLoginModal() {
    const modal = document.querySelector('[data-login-modal] [data-modal-target="container"]');
    if (modal) {
      modal.classList.remove('hidden');
      document.body.style.overflow = 'hidden';
    }
  }

  function openSignupModal() {
    const modal = document.querySelector('[data-signup-modal] [data-modal-target="container"]');
    if (modal) {
      modal.classList.remove('hidden');
      document.body.style.overflow = 'hidden';
    }
  }

  function closeAllModals() {
    document.querySelectorAll('[data-modal-target="container"]').forEach(modal => {
      modal.classList.add('hidden');
    });
    document.body.style.overflow = '';
  }

  // Allow closing modals with Escape key
  document.addEventListener('keydown', (e) => {
    if (e.key === 'Escape') {
      closeAllModals();
    }
  });
</script>
